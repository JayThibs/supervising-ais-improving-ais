{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "args": ["--model_family", "openai", "--model", "gpt-3.5-turbo", "--test_mode", "--reuse_data", "all"]
        }
    ]
}